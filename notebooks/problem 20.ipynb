   "source": [
    "from functools import reduce\n",
    "import operator\n",
    "\n",
    "\n",
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "\n",
    "reduce(operator.add, [int(x) for x in str(factorial(100))])"
   ]
