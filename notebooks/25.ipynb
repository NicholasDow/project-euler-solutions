    "\n",
    "\n",
    "def fib():\n",
    "    i = 2\n",
    "    num1 = 1\n",
    "    num2 = 1\n",
    "    while int(math.log10(num2))+1 != 3:\n",
    "        temp = num2\n",
    "        num2 = num1 + num2\n",
    "        num1 = temp\n",
    "        i+=1\n",
    "    return i"
